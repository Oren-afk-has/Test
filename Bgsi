local Bgsi = loadstring(game:HttpGet("https://raw.githubusercontent.com/idonthaveoneatm/Libraries/normal/quake/src"))()

--Window
local window = Bgsi:Window({
    Title = "Bgsi Script", 
    isMobile = false,
    Size = {
        X = 550,
        Y = 400
    }
})

--Tab1
local Main = window:Tab({
    Name = "Main",
    tabColor = Color3.fromRGB(180, 240, 67),
    Image = "rbxassetid://10734908793"
})

--Main Toggles
local isToggled = false

Main:Toggle({
    Name = "Auto Blow",
    Default = false,
    Callback = function(var)
        isToggled = var
        if isToggled then
            while isToggled do
                local args = {
                    [1] = "BlowBubble"
                }
                game:GetService("ReplicatedStorage"):WaitForChild("Shared"):WaitForChild("Framework"):WaitForChild("Network"):WaitForChild("Remote"):WaitForChild("Event"):FireServer(unpack(args))
                task.wait()
            end
        end
    end
})

local isToggled = false

Main:Toggle({
    Name = "Auto Sell",
    Default = false,
    Callback = function(var)
        isToggled = var

        if isToggled then
            while isToggled do
                local args = {
                    [1] = "SellBubble"
                }
                game:GetService("ReplicatedStorage"):WaitForChild("Shared"):WaitForChild("Framework"):WaitForChild("Network"):WaitForChild("Remote"):WaitForChild("Event"):FireServer(unpack(args))
                wait(0.1)
            end
        end
    end
})

local isToggled = false

Main:Toggle({
    Name = "Auto Collect Pickups",
    Default = false,
    Callback = function(var)
        isToggled = var

        if isToggled then
            while isToggled do
                local renderedObject = workspace.Rendered:GetChildren()[14]
                if renderedObject then
                    for _, coin in pairs(renderedObject:GetChildren()) do
                        game:GetService("ReplicatedStorage").Remotes.Pickups.CollectPickup:FireServer(coin.Name)
                        if coin and coin.Parent == renderedObject then
                            coin:Destroy()
                        end
                    end
                end
                task.wait()
            end
        end
    end
})
